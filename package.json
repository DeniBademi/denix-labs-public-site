{
  "name": "public-site",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve:bg": "ng serve --configuration=development-bg --port=4200",
    "serve:en": "ng serve --configuration=development-en --port=4201",
    "serve:both": "concurrently \"npm run serve:bg\" \"npm run serve:en\"",
    "build": "ng build",
    "build:bg": "ng build --configuration production-bg --output-path=dist/public-site/bg",
    "build:en": "ng build --configuration production-en --output-path=dist/public-site/en",
    "build:all": "npm run build:bg && npm run build:en",
    "build:production": "npm run build:all && npm run prepare-netlify",
    "prepare-netlify": "mkdir -p dist/public-site/browser && mkdir -p dist/public-site/browser/en && cp -r dist/public-site/bg/browser/bg/* dist/public-site/browser/ && cp -r dist/public-site/bg/browser/bg/en/* dist/public-site/browser/en/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:PublicSite": "node dist/public-site/server/server.mjs",
    "extract-i18n": "ng extract-i18n --output-path src/locale"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.0",
    "@angular/common": "^19.1.0",
    "@angular/compiler": "^19.1.0",
    "@angular/core": "^19.1.0",
    "@angular/forms": "^19.1.0",
    "@angular/platform-browser": "^19.1.0",
    "@angular/platform-browser-dynamic": "^19.1.0",
    "@angular/platform-server": "^19.1.0",
    "@angular/router": "^19.1.0",
    "@angular/ssr": "^19.1.8",
    "@netlify/angular-runtime": "2.4.0",
    "@nguniversal/express-engine": "^7.0.2",
    "@tailwindcss/postcss": "^4.0.12",
    "@tsparticles/angular": "^3.0.0",
    "@tsparticles/engine": "^3.8.1",
    "@tsparticles/slim": "^3.8.1",
    "@vapi-ai/web": "^2.3.0",
    "animate.css": "^4.1.1",
    "cookieconsent": "^3.1.1",
    "express": "^4.18.2",
    "flyonui": "^1.3.0",
    "ng-particles": "^3.12.0",
    "ngx-cookieconsent": "^8.0.0",
    "nouislider": "^15.8.1",
    "postcss": "^8.5.3",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.0.9",
    "tailwindcss-motion": "^1.1.0",
    "tslib": "^2.3.0",
    "tsparticles-engine": "^2.12.0",
    "tsparticles-slim": "^2.12.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.8",
    "@angular/cli": "^19.1.8",
    "@angular/compiler-cli": "^19.1.0",
    "@angular/localize": "^19.1.8",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.0",
    "jasmine-core": "~5.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.9",
    "tailwindcss-motion": "^1.1.0",
    "typescript": "~5.7.2"
  }
}
