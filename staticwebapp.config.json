{
  "routes": [
    {
      "route": "/*.{js,css,png,jpg,gif,svg,ttf,woff,woff2,eot,json}",
      "headers": {
        "cache-control": "must-revalidate, max-age=86400"
      }
    },
    {
      "route": "/",
      "redirect": "/en/index.html"
    },
    {
      "route": "/index.html",
      "rewrite": "/en/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/en/index.html",
    "exclude": [
      "/*.{js,css,png,jpg,gif,svg,ttf,woff,woff2,eot,json}",
      "/js/*",
      "/fonts/*",
      "/logos/*",
      "/icons/*"
    ]
  }
}