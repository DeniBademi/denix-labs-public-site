{
  "routes": [
    {
      "route": "*.{js,json,css,jpg,png,ico,webmanifest}"
    },
    {
      "route": "/",
      "redirect": "/en/index.html"
    },
    {
      "route": "/index.html",
      "rewrite": "/en/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/images/*.{png,jpg,gif,ico}",
      "/*.{css,scss,js,json,map,ico}",
      "/fonts/*",
      "/public/*"
    ]
  },
  "mimeTypes": {
    ".js": "application/javascript",
    ".css": "text/css",
    ".html": "text/html",
    ".json": "application/json",
    ".ico": "image/x-icon",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".gif": "image/gif",
    ".svg": "image/svg+xml",
    ".ttf": "font/ttf",
    ".woff": "font/woff",
    ".woff2": "font/woff2"
  }
}